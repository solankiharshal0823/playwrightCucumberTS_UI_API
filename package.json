    "@types/cucumber-html-reporter": "5.0.1",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonpath": "^0.2.4",
    "@types/randomstring": "^1.3.0",
    "@types/string-format": "^2.0.3",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-plugin-playwright": "1.6.2",
    "fetch-to-curl": "0.6.0",
    "fs-extra": "11.2.0",
    "jsonpath": "1.1.1",
    "moment": "2.30.1",
    "multiple-cucumber-html-reporter": "3.6.2",
    "os": "0.1.2",
    "playwright": "1.45.1",
    "randomstring": "1.3.0",
    "string-format": "2.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "winston": "3.13.1",
    "xml-formatter": "3.6.3",
    "xmldom": "0.6.0",
    "xpath": "0.0.34"
  }
